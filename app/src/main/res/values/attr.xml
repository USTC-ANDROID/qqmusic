<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="radius" format="dimension" />
    <declare-styleable name="RoundImageView">
        <attr name="radius" />
    </declare-styleable>
    <declare-styleable name="HeadView">
        <attr name="title" format="string"></attr>
        <attr name="leftImg" format="integer"></attr>
        <attr name="rightImg" format="integer"></attr>
    </declare-styleable>
    <declare-styleable name="Indicator">
        <attr name="color" format="color|reference" />
        <attr name="height" format="dimension|reference" />
    </declare-styleable>
    <declare-styleable name="Lrc">
        <attr name="android:textSize" />
        <attr name="normalTextColor" format="reference|color" />
        <attr name="currentTextColor" format="reference|color" />
        <attr name="dividerHeight" format="dimension|reference" />
        <attr name="rows" format="integer" />
    </declare-styleable>
</resources>